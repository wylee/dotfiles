Host github.com github
    HostName github.com
    IdentityFile ~/.ssh/id_rsa-github

Host bitbucket.org bitbucket
    HostName bitbucket.org
    IdentityFile ~/.ssh/id_rsa-bitbucket

Host pypi.python.org pypi
    HostName pypi.python.org
    IdentityFile ~/.ssh/id_rsa-pypi

Host static.bycycle.org
    User bycycle

Host hera.rc.pdx.edu hera
    HostName hera.rc.pdx.edu
    IdentityFile ~/.ssh/id_rsa
    ForwardAgent yes

Host hera-rsync
    HostName hera.rc.pdx.edu
    IdentityFile ~/.ssh/id_rsa-rsync

Host hrimfaxi.oit.pdx.edu hrimfaxi
    HostName hrimfaxi.oit.pdx.edu
    IdentityFile ~/.ssh/id_rsa
    ForwardAgent yes

Host merope.rc.pdx.edu merope
    HostName merope.rc.pdx.edu
    IdentityFile ~/.ssh/id_rsa
    ForwardAgent yes
    ProxyCommand ssh -W %h:%p hera.rc.pdx.edu
